<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bgs.reception.mapper.PictureSheetMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.bgs.reception.pojo.PictureSheet">
        <id column="id" property="id" />
        <result column="createTime" property="createTime" />
        <result column="album_name" property="albumName" />
        <result column="activityid" property="activityid" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, createTime, album_name, activityid
    </sql>

</mapper>
