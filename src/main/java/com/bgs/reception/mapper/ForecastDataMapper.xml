<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bgs.reception.mapper.ForecastDataMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.bgs.reception.pojo.ForecastData">
        <id column="id" property="id" />
        <result column="planName" property="planName" />
        <result column="planNumberTables" property="planNumberTables" />
        <result column="startTime" property="startTime" />
        <result column="endTime" property="endTime" />
        <result column="status" property="status" />
        <result column="entryTime" property="entryTime" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, planName, planNumberTables, startTime, endTime, status, entryTime
    </sql>

</mapper>
